<table>
	<thead>
		<tr>
			<th>Title</th>
			<th>Release Year</th>
			<th>Artist</th>
			<th>Genre</th>
		</tr>
	</thead>
	<tbody>
		<% for song in @songs %>
		<tr>
			<td><%= song.title %></td>
			<td><%= song.release_year %></td>
			<td><%= song.artist_name %></td>
			<td><%= song.genre %></td>
			<td><%= link_to "Delete", song, method: :delete, data: {confirm: "Are you sure?"} %></td>
		</tr>
		<% end %>
	</tbody>
</table>